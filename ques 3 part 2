#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;
    int arr[n-1];
    for (int i = 0; i < n-1; i++) cin >> arr[i];
    int low = 0, high = n - 2, ans = n;
    while (low <= high) {
        int mid = (low + high) / 2;
        if (arr[mid] == mid + 1) low = mid + 1;
        else {
            ans = mid + 1;
            high = mid - 1;
        }
    }
    cout << ans;
}
