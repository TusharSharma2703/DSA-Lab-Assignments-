#include <iostream>
using namespace std;

int main() {
    int n, key;
    cin >> n;
    int arr[n];
    for (int i = 0; i < n; i++) cin >> arr[i];
    cin >> key;
    int low = 0, high = n - 1, mid, pos = -1;
    while (low <= high) {
        mid = (low + high) / 2;
        if (arr[mid] == key) {
            pos = mid;
            break;
        }
        else if (arr[mid] < key) low = mid + 1;
        else high = mid - 1;
    }
    if (pos == -1) cout << "Not Found";
    else cout << "Found at index " << pos;
}
